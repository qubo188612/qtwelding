<ETHERNETKRL>
	<CONFIGURATION>
		<EXTERNAL>
			<!--配置下位机作为服务器-->
			<TYPE>Client</TYPE>
		</EXTERNAL>
		<INTERNAL>
			<ENVIRONMENT>Program</ENVIRONMENT>
 			<BUFFERING Mode="FIFO" Limit="512"/>
 			<BUFFSIZE Limit="16384"/>
 			<TIMEOUT Connect="60000"/>
			<ALIVE Set_Out="700" Ping="701"/>
 			<IP>192.168.1.123</IP>
 			<PORT>54611</PORT>
 			<PROTOCOL>TCP</PROTOCOL>
 			<MESSAGES Logging="error" Display="disabled"/>
		</INTERNAL>
	</CONFIGURATION>
	<RECEIVE>
		<XML>
			<!--机器人位姿数据-->
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/X" Type="REAL"/><!--直线运动时，终点坐标 圆弧运动的第一个点-->
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/Y" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/Z" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RX" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RY" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RZ" Type="REAL"/>
			
			<!--变位机-->
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT1" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT2" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT3" Type="INT"/>

			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/X1" Type="REAL"/> <!--机器人圆弧运动时，中点坐标-->
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/Y1" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/Z1" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RX1" Type="REAL"/> 
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RY1" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/RZ1" Type="REAL"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT11" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT21" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/MOVEPOSE/OUT31" Type="INT"/>

			<ELEMENT Tag="ROBOTCONTROL/IO/OUT1" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT2" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT3" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT4" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT5" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT6" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT7" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/IO/OUT8" Type="INT"/>

			<ELEMENT Tag="ROBOTCONTROL/AO/OUT1" Type="REAL"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO/OUT2" Type="REAL"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO/OUT3" Type="REAL"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO/OUT4" Type="REAL"/>

			<!--机器人移动方式 0：直线运动，1：关节运动，2：圆弧运动，3：密集点运动-->
			<ELEMENT Tag="ROBOTCONTROL/MOVEMODE" Type="INT"/>

			<!--电流 -->
			<ELEMENT Tag="ROBOTCONTROL/ELED" Type="REAL"/>

			<!--焊接模式-->
			<ELEMENT Tag="ROBOTCONTROL/ELEM" Type="INT"/>

			<!--速度-->
			<ELEMENT Tag="ROBOTCONTROL/MOVESPEED" Type="REAL"/>
 
			<!--TCP -->
			<ELEMENT Tag="ROBOTCONTROL/TCP" Type="INT"/>

			<!--起弧-->
			<ELEMENT Tag="ROBOTCONTROL/WELD" Type="INT"/>

			<!--运行状态 0：允许运行，1：暂停（熄弧），2：继续运行，3：，-->
			<ELEMENT Tag="ROBOTCONTROL/RUNSTATE" Type="INT"/>

			<!--IO -->
			<ELEMENT Tag="ROBOTCONTROL/AO1" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO2" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO3" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/AO4" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO1" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO2" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO3" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO4" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/DO5" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO6" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO7" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DO8" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI1" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI2" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI3" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI4" Type="INT"/>
			<ELEMENT Tag="ROBOTCONTROL/DI5" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI6" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI7" Type="INT"/> 
			<ELEMENT Tag="ROBOTCONTROL/DI8" Type="INT"/>
			
			<ELEMENT Tag="ROBOTCONTROL" Set_Flag="99"/> <!--接收到数据后利用99标志触发89中断读取数据-->
		</XML>
	</RECEIVE>
	<SEND>
		<XML>
 			
 		</XML>
	</SEND> 
</ETHERNETKRL>